{
    "name": "rimbaborne/moota",
    "description": "Update API Moota V2 From yugo/moota Unofficial Moota.co package for Laravel framework",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gagang",
            "email": "rimbaborne@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Rimbaborne\\Moota\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rimbaborne\\Moota\\Providers\\MootaServiceProvider"
            ],
            "aliases": {
                "Moota": "Rimbaborne\\Moota\\Facades\\Moota"
            }
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": ">=5.2"
    }
}
